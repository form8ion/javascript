{
  "name": "@form8ion/javascript",
  "description": "JavaScript language plugin for the @form8ion toolset",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^16.14.0 || >=18.0.0"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "repository": "form8ion/javascript",
  "bugs": "https://github.com/form8ion/javascript/issues",
  "homepage": "https://npm.im/@form8ion/javascript",
  "runkitExampleFilename": "./example.js",
  "exports": {
    "module": "./lib/index.mjs",
    "require": "./lib/index.js",
    "import": "./lib/index.mjs"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "sideEffects": false,
  "scripts": {
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "lint:publish": "publint --strict",
    "generate:md": "remark . --output",
    "pregenerate:md": "run-s build",
    "prepare": "husky install",
    "lint:gherkin": "gherkin-lint",
    "pretest:integration:base": "run-s build",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=\"--loader=testdouble --enable-source-maps\" DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:wip:debug": "DEBUG=test run-s 'test:integration:wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "npm-run-all --print-label build --parallel test:unit:mocha test:unit:vitest",
    "test:unit:vitest": "DEBUG=any vitest run",
    "test:unit:mocha": "DEBUG=any mocha 'src/**/*-test.js'"
  },
  "files": [
    "example.js",
    "lib/",
    "templates/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/codecov": "^5.0.0",
    "@form8ion/commit-convention": "^6.0.0",
    "@form8ion/config-file": "^1.0.1",
    "@form8ion/core": "^3.0.0",
    "@form8ion/eslint": "^5.0.0",
    "@form8ion/execa-wrapper": "^1.0.0-alpha.1",
    "@form8ion/husky": "^5.0.0",
    "@form8ion/javascript-core": "^10.0.0",
    "@form8ion/overridable-prompts": "^1.2.0",
    "@form8ion/prettier": "^2.0.0",
    "@form8ion/rollup": "^3.0.0",
    "@hapi/hoek": "^11.0.0",
    "@travi/cli-messages": "^1.1.0",
    "@travi/language-scaffolder-prompts": "^1.3.0",
    "camelcase": "^6.2.1",
    "deepmerge": "^4.2.2",
    "filedirname": "^2.7.0",
    "ini": "^4.0.0",
    "joi": "^17.5.0",
    "make-dir": "^4.0.0",
    "mustache": "^4.2.0",
    "npm-conf": "^1.1.3",
    "sort-object-keys": "^1.1.3",
    "touch": "^3.1.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.22.15",
    "@cucumber/cucumber": "9.6.0",
    "@form8ion/babel-preset": "1.6.117",
    "@form8ion/commitlint-config": "1.0.65",
    "@form8ion/eslint-config": "7.0.5",
    "@form8ion/eslint-config-cucumber": "1.4.1",
    "@form8ion/eslint-config-mocha": "3.0.1",
    "@form8ion/remark-lint-preset": "6.0.2",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@travi/any": "3.0.2",
    "ban-sensitive-files": "1.10.1",
    "c8": "8.0.1",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "8.0.3",
    "jest-when": "3.6.0",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.12.1",
    "ls-engines": "0.9.1",
    "mocha": "10.2.0",
    "mock-fs": "5.2.0",
    "npm-run-all2": "6.1.1",
    "publint": "0.2.5",
    "remark-cli": "12.0.0",
    "remark-toc": "9.0.0",
    "remark-usage": "11.0.1",
    "rimraf": "5.0.5",
    "rollup": "2.79.1",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "17.0.1",
    "testdouble": "3.20.1",
    "vitest": "0.34.6"
  }
}

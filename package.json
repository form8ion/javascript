{
  "name": "@form8ion/javascript",
  "description": "JavaScript language plugin for the @form8ion toolset",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": ">=12.20"
  },
  "files": [
    "example.js",
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "repository": "form8ion/javascript",
  "bugs": "https://github.com/form8ion/javascript/issues",
  "homepage": "https://npm.im/@form8ion/javascript",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "generate:md": "remark . --output",
    "pregenerate:md": "run-s build",
    "prepare": "husky install",
    "pretest:integration": "preview",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:wip:debug": "DEBUG=test run-s 'test:integration:wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "cross-env NODE_ENV=test nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'"
  },
  "devDependencies": {
    "@babel/register": "7.16.0",
    "@cucumber/cucumber": "7.3.1",
    "@form8ion/babel-preset": "1.6.71",
    "@form8ion/commitlint-config": "1.0.22",
    "@form8ion/eslint-config": "1.8.5",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/eslint-config-mocha": "1.2.12",
    "@form8ion/remark-lint-preset": "2.1.6",
    "@istanbuljs/nyc-config-babel": "3.0.0",
    "@rollup/plugin-babel": "5.3.0",
    "@travi/any": "2.0.18",
    "babel-plugin-istanbul": "6.1.1",
    "ban-sensitive-files": "1.9.16",
    "chai": "4.3.4",
    "codecov": "3.8.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "7.0.4",
    "lockfile-lint": "4.6.2",
    "ls-engines": "0.6.1",
    "mocha": "9.1.3",
    "mock-fs": "5.1.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "package-preview": "4.0.0",
    "remark-cli": "10.0.0",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.59.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "12.0.1"
  },
  "dependencies": {
    "@form8ion/javascript-core": "^3.1.0",
    "@form8ion/overridable-prompts": "^1.1.1",
    "@hapi/joi": "^17.1.1",
    "deepmerge": "^4.2.2",
    "inquirer": "^8.2.0"
  }
}

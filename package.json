{
  "name": "@form8ion/javascript",
  "description": "JavaScript language plugin for the @form8ion toolset",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^18.17.0 || >=20.6.1"
  },
  "type": "module",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "repository": "form8ion/javascript",
  "bugs": "https://github.com/form8ion/javascript/issues",
  "homepage": "https://npm.im/@form8ion/javascript",
  "runkitExampleFilename": "./example.js",
  "exports": {
    "module": "./lib/index.js",
    "import": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "temp-disabled:lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "lint:publish": "publint --strict",
    "generate:md": "remark . --output",
    "pregenerate:md": "run-s build",
    "prepare": "husky install",
    "lint:gherkin": "gherkin-lint",
    "pretest:integration:base": "run-s build",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=\"$OPTIONS --enable-source-maps\" DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:wip:debug": "DEBUG=test run-s 'test:integration:wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any vitest run"
  },
  "files": [
    "example.js",
    "lib/",
    "templates/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/codecov": "^6.0.0",
    "@form8ion/commit-convention": "^7.0.0",
    "@form8ion/config-file": "^1.1.1",
    "@form8ion/core": "^4.0.0",
    "@form8ion/eslint": "^6.1.0",
    "@form8ion/execa-wrapper": "^1.0.0-alpha.1",
    "@form8ion/husky": "^5.3.0",
    "@form8ion/javascript-core": "^12.0.0-beta.1",
    "@form8ion/overridable-prompts": "^1.2.0",
    "@form8ion/prettier": "^2.0.0",
    "@hapi/hoek": "^11.0.0",
    "@travi/cli-messages": "^1.1.0",
    "@travi/language-scaffolder-prompts": "^2.0.0-beta.1",
    "camelcase": "^8.0.0",
    "deepmerge": "^4.2.2",
    "filedirname": "^3.0.0",
    "ini": "^5.0.0",
    "joi": "^17.5.0",
    "make-dir": "^4.0.0",
    "mustache": "^4.2.0",
    "npm-conf": "^1.1.3",
    "sort-object-keys": "^1.1.3",
    "touch": "^3.1.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "10.9.0",
    "@form8ion/commitlint-config": "1.0.76",
    "@form8ion/eslint-config": "7.0.11",
    "@form8ion/eslint-config-cucumber": "1.4.1",
    "@form8ion/eslint-config-mocha": "3.0.6",
    "@form8ion/remark-lint-preset": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@travi/any": "3.1.2",
    "ban-sensitive-files": "1.10.6",
    "c8": "10.1.2",
    "chai": "5.1.1",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.4",
    "husky": "9.1.5",
    "jest-when": "3.6.0",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.14.0",
    "ls-engines": "0.9.3",
    "mock-fs": "5.2.0",
    "npm-run-all2": "6.2.2",
    "publint": "0.2.10",
    "remark-cli": "12.0.1",
    "remark-toc": "9.0.0",
    "remark-usage": "11.0.1",
    "rimraf": "6.0.1",
    "rollup": "4.22.2",
    "rollup-plugin-auto-external": "2.0.0",
    "testdouble": "3.20.2",
    "vitest": "2.0.5"
  },
  "packageManager": "npm@10.8.3"
}

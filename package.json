{
  "name": "@form8ion/javascript",
  "description": "JavaScript language plugin for the @form8ion toolset",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": ">=12.20"
  },
  "files": [
    "example.js",
    "lib/",
    "templates/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "repository": "form8ion/javascript",
  "bugs": "https://github.com/form8ion/javascript/issues",
  "homepage": "https://npm.im/@form8ion/javascript",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "generate:md": "remark . --output",
    "pregenerate:md": "run-s build",
    "prepare": "husky install",
    "lint:gherkin": "gherkin-lint",
    "pretest:integration:base": "preview",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:wip:debug": "DEBUG=test run-s 'test:integration:wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'"
  },
  "devDependencies": {
    "@babel/register": "7.17.0",
    "@cucumber/cucumber": "7.3.2",
    "@form8ion/babel-preset": "1.6.80",
    "@form8ion/commitlint-config": "1.0.29",
    "@form8ion/eslint-config": "2.0.3",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/eslint-config-mocha": "1.2.17",
    "@form8ion/remark-lint-preset": "2.1.7",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@travi/any": "2.0.19",
    "ban-sensitive-files": "1.9.17",
    "c8": "7.11.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "clear-module": "4.1.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "7.0.4",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.6.2",
    "ls-engines": "0.6.5",
    "mocha": "9.2.0",
    "mock-fs": "5.1.2",
    "npm-run-all": "4.1.5",
    "package-preview": "4.0.0",
    "remark-cli": "10.0.1",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.67.2",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "13.0.1",
    "testdouble": "3.16.4"
  },
  "dependencies": {
    "@form8ion/codecov": "^2.0.1",
    "@form8ion/commit-convention": "^1.7.0",
    "@form8ion/core": "^1.5.0",
    "@form8ion/eslint": "^2.0.1",
    "@form8ion/husky": "^2.2.1",
    "@form8ion/javascript-core": "^4.0.0",
    "@form8ion/overridable-prompts": "^1.1.1",
    "@form8ion/rollup": "^1.1.0",
    "@hapi/hoek": "^9.2.1",
    "@travi/cli-messages": "^1.0.5",
    "@travi/language-scaffolder-prompts": "^1.3.0",
    "camelcase": "^6.2.1",
    "deepmerge": "^4.2.2",
    "execa": "^5.1.1",
    "ini": "^2.0.0",
    "inquirer": "^8.2.0",
    "joi": "^17.5.0",
    "make-dir": "^3.1.0",
    "mustache": "^4.2.0",
    "npm-conf": "^1.1.3",
    "touch": "^3.1.0",
    "validate-npm-package-name": "^3.0.0"
  }
}
